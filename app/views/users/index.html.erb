<h1>This is the default User page</h1>
<table>
    <tr>
        <th>Author</th>
        <th>Email</th>
        <th>Actions avaliable</th>
    </tr>
    <%@users.each do |user|%>
    <tr>
        <td><%=user.username%></td>
        <td><%=user.email%></td>
        <td><%=link_to 'Edit', edit_user_path(user)%></td>
        <td><%=link_to 'Show', user_path(user)%></td>
        <td><%= button_to 'Delete', user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <%end%>
</table>